{
  "compilerOptions": {
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./src",
    "paths": {
      "@Components": ["Components/*"],
      "@Pages/*": ["Pages/*"],
      "@Context/*": ["Context/*"],
      "@Routes": ["Routes"],
      "@Routes/*": ["Routes/*"],
      "@API/*": ["Api/*"],
      "@Utils/*": ["Utils/*"],
      "~/*": ["Styles/*"],
      "@/*": ["*"]
    }
  },
  "exclude": ["node_modules", "public"]
}
